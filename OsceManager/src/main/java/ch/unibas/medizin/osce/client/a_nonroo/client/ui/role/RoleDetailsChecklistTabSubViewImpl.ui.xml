<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:ch.unibas.medizin.osce.client.style.widgets">
	<ui:style src="../../styles.css">
		
	</ui:style>
	
	<g:HTMLPanel addStyleNames="{style.patientDetails}">
		<g:VerticalPanel width="100%">
			
			<g:HorizontalPanel spacing="5" ui:field="addTopicHP">		
				<s:IconButton icon="plusthick" ui:field="addCheckListSectionButton"></s:IconButton>
				<s:IconButton icon="pencil" ui:field="editChecklistSectionButton"></s:IconButton>
				<s:IconButton icon="arrowthickstop-1-s" ui:field="importSectionButton" ></s:IconButton>
				<s:IconButton icon="trash" ui:field="deleteTab"></s:IconButton>
			</g:HorizontalPanel>
			
			<g:AbsolutePanel ui:field="checkListAP">
				<g:VerticalPanel ui:field="containerVerticalPanel" width="100%" spacing="3">
				</g:VerticalPanel>
			</g:AbsolutePanel>
		</g:VerticalPanel>
	</g:HTMLPanel>	
</ui:UiBinder> 