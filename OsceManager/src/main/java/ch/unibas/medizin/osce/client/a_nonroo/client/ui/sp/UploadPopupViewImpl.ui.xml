<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
  xmlns:r='urn:import:ch.unibas.medizin.osce.client.scaffold.ui'
  xmlns:a='urn:import:ch.unibas.medizin.osce.client.managed.ui'
  xmlns:my='urn:import:ch.unibas.medizin.osce.client.style.widgets'
  xmlns:b='urn:import:com.google.gwt.user.cellview.client'
  xmlns:n='urn:import:ch.unibas.medizin.osce.client.a_nonroo.client.ui.role'>

<ui:style src="../../styles.css" />


 <ui:with type="ch.unibas.medizin.osce.client.style.resources.UiIcons" field="uiIcons" ></ui:with>	
<g:HTMLPanel>
<div class="arrow-popup-border"></div>
<div class="arrow-popup"></div>

<g:HorizontalPanel spacing='10'>
			<g:FormPanel ui:field="uploadFormPanel" >
			<g:VerticalPanel spacing="5" horizontalAlignment="center" verticalAlignment="middle">
			
			<g:Label ui:field="uploadLbl" styleName="{style.label}"> Image Upload: </g:Label>
	        <g:HorizontalPanel>
	            
	            <g:FileUpload ui:field="fileUpload" name="fileUpload1" />
	            <g:TextBox ui:field="id" ></g:TextBox>
				<g:TextBox ui:field="name" ></g:TextBox>

	        </g:HorizontalPanel>
	        </g:VerticalPanel>
		</g:FormPanel>
	
 </g:HorizontalPanel>
</g:HTMLPanel> 
</ui:UiBinder>
