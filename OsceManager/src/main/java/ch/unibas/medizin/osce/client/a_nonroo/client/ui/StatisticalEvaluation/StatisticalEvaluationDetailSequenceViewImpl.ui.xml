							</td>
							<td width="13%">
								<g:HorizontalPanel ui:field="fourthColumnHP">
									<g:Label ui:field="sumPerSequenceLbl"></g:Label>
								</g:HorizontalPanel>
							</td>
						</tr>
						
						</tbody>
					</table>
				</g:HTMLPanel>
		
