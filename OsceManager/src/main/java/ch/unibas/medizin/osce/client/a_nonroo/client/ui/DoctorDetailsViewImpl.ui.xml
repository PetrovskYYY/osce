<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:style src="../styles.css" />

  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
      <tr id="gender">
                <td>
                    <span class="{style.label}">Gender:</span>
                </td>
                <td>
                    <span ui:field="gender"/>
                </td>
            </tr>
      <tr id="title">
                <td>
                    <span class="{style.label}">Title:</span>
                </td>
                <td>
                    <span ui:field="title"/>
                </td>
            </tr>
      <tr id="name">
                <td>
                    <span class="{style.label}">Name:</span>
                </td>
                <td>
                    <span ui:field="name"/>
                </td>
            </tr>
      <tr id="preName">
                <td>
                    <span class="{style.label}">Pre Name:</span>
                </td>
                <td>
                    <span ui:field="preName"/>
                </td>
            </tr>
      <tr id="email">
                <td>
                    <span class="{style.label}">Email:</span>
                </td>
                <td>
                    <span ui:field="email"/>
                </td>
            </tr>
      <tr id="telephone">
                <td>
                    <span class="{style.label}">Telephone:</span>
                </td>
                <td>
                    <span ui:field="telephone"/>
                </td>
            </tr>
      <tr id="clinic">
                <td>
                    <span class="{style.label}">Clinic:</span>
                </td>
                <td>
                    <span ui:field="clinic"/>
                </td>
            </tr>
      <tr id="office">
      	<g:SimplePanel ui:field="officePanel"></g:SimplePanel>
            </tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
