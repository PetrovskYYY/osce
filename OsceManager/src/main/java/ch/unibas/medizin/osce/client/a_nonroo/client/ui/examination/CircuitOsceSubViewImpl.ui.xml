<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets"
	xmlns:s="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">

	<ui:style src="../../styles.css">
		.padleft25 {
			padding-left: 25px;
		}
		
		.padleft33 {
			padding-left: 33px;
		}
		
		.padleft30 {
			padding-left: 30px;
		}
		
		.padleft8 {
			padding-left: 8px;
		}
		
		.marginRight20 {
			margin-right: 20px;
		}
		.marginLeft20
		{
			margin-Left: 20px;
		}
		
		
		.padLeft0
		{
			padding-left: 0px;
		}
		.padLeft10
		{
			padding-left: 10px;
		}
		.padLeft5
		{
			padding-left: 5px;
		}
		.padLeft43
		{
			padding-left: 43px;
		}
		.padLeft15
		{
			padding-left: 15px;
		}
		.marginRight5
		{
			margin-right: 5px;
		}
		.marginLeft30
		{
			margin-left: 30px;
		}
		.buttonStyle
		{
			width:80px;
		}
		
	</ui:style>
	<g:HTMLPanel>
		<!-- h1>CircuitOsceSubView</h1 -->
		<Table>
			<tr>
				<td><g:Label ui:field="shortBreakLabel" styleName="{style.padLeft0}">Short Break :</g:Label></td>
				<td><s:ShortBox ui:field="shortBreakTextBox"></s:ShortBox></td>
				<td><g:Label ui:field="longBreakLabel" styleName="{style.padLeft5}">Long Break :</g:Label></td>
				<td><s:ShortBox ui:field="longBreakTextBox"></s:ShortBox></td>
				<td><g:Label ui:field="launchBreakLabel" styleName="{style.padLeft15}">Launch Break:</g:Label></td>
				<td><s:ShortBox ui:field="launchBreakTextBox"></s:ShortBox></td>				
				
				<!--  Module 5 bug Report Change -->
				<td><g:Label ui:field="middleBreakLabel" styleName="{style.padLeft43}">Middle Break:</g:Label></td>
				<td><s:ShortBox ui:field="middleBreakTextBox"></s:ShortBox></td>
				<!--  E Module 5 bug Report Change -->
				
				<td><my:IconButton ui:field="saveOsce" icon="plusthick"></my:IconButton></td>
				<td><g:Button ui:field="generateBtn" styleName="{style.buttonStyle}"></g:Button></td>
				<td><g:Button ui:field="closedBtn" styleName="{style.buttonStyle}"></g:Button></td>	
			</tr>
			<tr>
				<td><g:Label ui:field="maxStudentLabel"  styleName="{style.padLeft0}">Max Students :</g:Label></td>
				<td><g:IntegerBox ui:field="maxStudentTextBox"></g:IntegerBox></td>
				<td><g:Label ui:field="maxParcourLabel" styleName="{style.padLeft5}">Max Parcour :</g:Label></td>
				<td><g:IntegerBox ui:field="maxParcourTextBox"></g:IntegerBox></td>
				<td><g:Label ui:field="maxRoomsLabel" styleName="{style.padLeft5}">Max No Rooms :</g:Label></td>
				<td><g:IntegerBox ui:field="maxRoomsTextBox"></g:IntegerBox></td>
					
				<!--  Module 5 bug Report Change -->
				<td><g:Label ui:field="shortBreakSimpatLabel" styleName="{style.padLeft5}">Short Break Simpat:</g:Label></td>
				<td><s:ShortBox ui:field="shortBreakSimpatTextBox"></s:ShortBox></td>
				<!--  E Module 5 bug Report Change -->
				
				<td><g:Button ui:field="clearAllBtn" styleName="{style.buttonStyle}"></g:Button></td>
				<td><g:Button ui:field="fixedBtn" styleName="{style.buttonStyle}"></g:Button></td>				
				<td><g:Label></g:Label></td>
			</tr>
		</Table>
	</g:HTMLPanel>
</ui:UiBinder> 