<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:i="urn:import:ch.unibas.medizin.osce.client.style.widgetsnewcustomsuggestbox.test.client.ui.widget.suggest.impl" xmlns:s="urn:import:ch.unibas.medizin.osce.client.style.widgets">
	<ui:style src="../../styles.css">
		
	</ui:style>
	<ui:with type="ch.unibas.medizin.osce.shared.i18n.OsceConstants" field="constants"></ui:with>
	
	<g:VerticalPanel spacing="5">
		<g:HTMLPanel>
			<table spacing="8">
				<tr>
					<td>
						<g:Label ui:field="specialisationLbl" text="{constants.manualOsceSpecialisation}"></g:Label>	
					</td>
					<td>
						<i:DefaultSuggestBox ui:field="specialisationSuggestBox"></i:DefaultSuggestBox>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="roleTopicLbl" text="{constants.manualOsceRoleTopic}"></g:Label>
					</td>
					<td>
						<i:DefaultSuggestBox ui:field="roleTopicSuggestBox"></i:DefaultSuggestBox>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="standardizedRoleLbl" text="{constants.manualOsceStandardizedRole}"></g:Label>
					</td>
					<td>
						<i:DefaultSuggestBox ui:field="standardizedRoleSuggestBox"></i:DefaultSuggestBox>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="roomLbl" text="{constants.manualOsceCreateRoom}"></g:Label>
					</td>
					<td>
						<i:DefaultSuggestBox ui:field="roomSuggestBox"></i:DefaultSuggestBox>
					</td>
				</tr>
				<tr>
					<td colspan="2" align="center">
						<s:IconButton ui:field="okBtn" icon="check" text="{constants.okBtn}"></s:IconButton>
						<s:IconButton ui:field="closeBtn" icon="closethick" text="{constants.cancel}"></s:IconButton>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:VerticalPanel>
</ui:UiBinder> 