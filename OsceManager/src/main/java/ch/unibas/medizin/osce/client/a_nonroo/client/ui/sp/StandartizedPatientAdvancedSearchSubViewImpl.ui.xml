<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	 xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets">
	 
	<ui:style src="../../styles.css" />
	
	
<g:HTMLPanel styleName="{style.listView}">
    <div style="height:3em;"/>
    <b:CellTable pageSize="8" ui:field="table" width="100%"/>
    <div class="{style.controls}">      
      <g:Button  ui:field="newButton">Create AdvancedSearchCriteria</g:Button>
      <b:SimplePager display="{table}" location="RIGHT"/>
    </div>
  </g:HTMLPanel>
</ui:UiBinder> 