<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="numberRotation">
            <div class="{style.label}">Number Rotation</div>
            <div class="{style.value}" ui:field="numberRotation"/>
        </div>
    <div id="label">
            <div class="{style.label}">Label</div>
            <div class="{style.value}" ui:field="label"/>
        </div>
    <div id="osceDay">
            <div class="{style.label}">Osce Day</div>
            <div class="{style.value}" ui:field="osceDay"/>
        </div>
    <div id="courses">
            <div class="{style.label}">Courses</div>
            <div class="{style.value}" ui:field="courses"/>
        </div>
    <div id="oscePosts">
            <div class="{style.label}">Osce Posts</div>
            <div class="{style.value}" ui:field="oscePosts"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
