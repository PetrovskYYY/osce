<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="../../styles.css" />
		
	<g:HTMLPanel>
		<div style="height:3em;" />
		<b:CellTable pageSize="8" ui:field="table" width="100%" />
		<div class="{style.controls}">
			<g:ValueListBox ui:field="scarBox"></g:ValueListBox>
			<g:Button ui:field="scarAddButton">add</g:Button>
			<b:SimplePager display="{table}" location="RIGHT" />
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 