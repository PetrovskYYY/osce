<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets"
	xmlns:d="urn:import:com.google.gwt.dom.client" xmlns:S="urn:import:ch.unibas.medizin.osce.client.a_nonroo.client.ui.StatisticalEvaluation">

	<ui:style src="../../styles.css">
		.panelStyle {
			width: 320px;
			height: 400px;
			padding: 0px;
			margin: 10px 10px 0;
		}
				
		.commonStyle {
			margin: 5px 10px 5px 20px;
			width: 190px;
		}
		
		.topAlign {
			vertical-align: top;
		}
		
		.scrollpanelStyle
		{
			height:400px; 
			width:320px;				
		}
		
	</ui:style>


	<g:HTMLPanel>	
		<g:ScrollPanel  ui:field="scrollPanel" >
		<g:HTMLPanel width="100%">	
						
		    			<div style="float:left; padding-right:10px;">
							
								
								
								<g:Button ui:field="exportBtn"></g:Button>
								
								<g:Button ui:field="newexportBtn"></g:Button>
							
						</div>
						<div style="float:right;">
							<g:HorizontalPanel spacing="5" horizontalAlignment="right" >
								<my:FocusableValueListBox ui:field="analysisListBox"></my:FocusableValueListBox>
								<g:Button ui:field="calculateBtn"></g:Button>
							</g:HorizontalPanel>
						</div>
						<br /><br />
						<div style="width:100%">
							<g:VerticalPanel ui:field="sequenceVP" horizontalAlignment="center" addStyleNames="{style.sequenceVP}">
							<!--  	<S:StatisticalEvaluationDetailSequenceViewImpl></S:StatisticalEvaluationDetailSequenceViewImpl> -->
							</g:VerticalPanel>
						</div>
					
				<!--  <g:Button ui:field="importBtn"></g:Button> -->
			
			
			</g:HTMLPanel>
		</g:ScrollPanel>	
	</g:HTMLPanel>
	
</ui:UiBinder> 