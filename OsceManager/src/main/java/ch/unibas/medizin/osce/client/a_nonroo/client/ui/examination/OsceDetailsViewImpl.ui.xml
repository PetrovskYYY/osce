					<my:IconButton ui:field="delete" icon="trash">Delete</my:IconButton>
					<my:IconButton ui:field="exportXml" icon="extlink"></my:IconButton>
					<my:IconButton ui:field="exportSettingsQRCode" icon="extlink"></my:IconButton>

				</div>
			</g:HTMLPanel>
		</g:HorizontalPanel>


