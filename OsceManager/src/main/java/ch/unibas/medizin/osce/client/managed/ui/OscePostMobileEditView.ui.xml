<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.unibas.medizin.osce.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
        
    <div id="isPossibleStart">
            <div class="{style.label}">Is Possible Start</div>
            <div class="{style.value}">
                <g:CheckBox ui:field="isPossibleStart"/>
            </div>
        </div>
        <div id="oscePostBlueprint">
            <div class="{style.label}">Osce Post Blueprint</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="oscePostBlueprint"/>
            </div>
        </div>
        <div id="oscePostRooms">
            <div class="{style.label}">Osce Post Rooms</div>
            <div class="{style.value}">
                <a:OscePostRoomSetEditor ui:field="oscePostRooms"/>
            </div>
        </div>
        <div id="patientInRole">
            <div class="{style.label}">Patient In Role</div>
            <div class="{style.value}">
                <a:PatientInRoleSetEditor ui:field="patientInRole"/>
            </div>
        </div>
        <div id="standardizedRole">
            <div class="{style.label}">Standardized Role</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="standardizedRole"/>
            </div>
        </div>
        <div id="osceSequence">
            <div class="{style.label}">Osce Sequence</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="osceSequence"/>
            </div>
        </div>
        <div id="sequenceNumber">
            <div class="{style.label}">Sequence Number</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="sequenceNumber"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
