<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.unibas.medizin.osce.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
    
        
        
        
        
    <div id="osceDate">
            <div class="{style.label}">Osce Date</div>
            <div class="{style.value}">
                <d:DateBox ui:field="osceDate"/>
            </div>
        </div>
        <div id="timeStart">
            <div class="{style.label}">Time Start</div>
            <div class="{style.value}">
                <d:DateBox ui:field="timeStart"/>
            </div>
        </div>
        <div id="timeEnd">
            <div class="{style.label}">Time End</div>
            <div class="{style.value}">
                <d:DateBox ui:field="timeEnd"/>
            </div>
        </div>
        <div id="osce">
            <div class="{style.label}">Osce</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="osce"/>
            </div>
        </div>
        <div id="osceSequences">
            <div class="{style.label}">Osce Sequences</div>
            <div class="{style.value}">
                <a:OsceSequenceSetEditor ui:field="osceSequences"/>
            </div>
        </div>
        <div id="patientInSemesters">
            <div class="{style.label}">Patient In Semesters</div>
            <div class="{style.value}">
                <a:PatientInSemesterSetEditor ui:field="patientInSemesters"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
