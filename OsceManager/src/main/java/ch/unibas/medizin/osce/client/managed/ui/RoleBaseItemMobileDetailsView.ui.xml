<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='item_defination'><div class='{style.label}'>Item_defination</div><div class='{style.value}' ui:field='item_defination'></div></div>
    <div id='item_name'><div class='{style.label}'>Item_name</div><div class='{style.value}' ui:field='item_name'></div></div>
    <div id='deleted'><div class='{style.label}'>Deleted</div><div class='{style.value}' ui:field='deleted'></div></div>
    <div id='roleTemplate'><div class='{style.label}'>Role Template</div><div class='{style.value}' ui:field='roleTemplate'></div></div>
    <div id='sort_order'><div class='{style.label}'>Sort_order</div><div class='{style.value}' ui:field='sort_order'></div></div>
    <div id='roleTableItem'><div class='{style.label}'>Role Table Item</div><div class='{style.value}' ui:field='roleTableItem'></div></div>
    <div id='roleItemAccess'><div class='{style.label}'>Role Item Access</div><div class='{style.value}' ui:field='roleItemAccess'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
