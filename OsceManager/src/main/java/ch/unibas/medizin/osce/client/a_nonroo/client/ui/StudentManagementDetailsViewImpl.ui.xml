<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	 xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets"
	 xmlns:r="urn:import:ch.unibas.medizin.osce.client.style.resources">
	 <ui:style src="../styles.css" >
		.scrollAuto1{

	
		}
		.containerPanel{
			margin-left: 20px;
			width: 100%;
			height: 100%;
		}	
	 </ui:style>
	
	

<g:HTMLPanel height="100%" width="100%">

	<g:TabPanel ui:field="studentDetailPanel" width="100%" height="100%">
	<g:Tab ui:field="tab1">
		<g:TabHTML>General Information</g:TabHTML>
		<g:HTMLPanel height="100%">
		<g:HorizontalPanel  ui:field="generalInformationHorizontalPanel">
		<g:VerticalPanel spacing="15" ui:field="generalInformationHorizontalVerticalPanel1">
		<g:Label>Name : </g:Label>
		<g:Label>Prename : </g:Label>
		<g:Label>Street : </g:Label>
		<g:Label>City : </g:Label>
		<g:Label>Gender : </g:Label>
		<g:Label>Email : </g:Label>
		</g:VerticalPanel>
		<g:VerticalPanel spacing="15" ui:field="generalInformationHorizontalVerticalPanel2">
		<g:Label ui:field="Name"></g:Label>
		<g:Label ui:field="Prename"></g:Label>
		<g:Label ui:field="Street"></g:Label>
		<g:Label ui:field="City"></g:Label>
		<g:Label ui:field="Gender"></g:Label>
		<g:Label ui:field="Email"></g:Label>
		</g:VerticalPanel>
		</g:HorizontalPanel>
		</g:HTMLPanel>
	</g:Tab>
	<g:Tab ui:field="tab2">
		<g:TabHTML>Osce Participation</g:TabHTML>
		<g:HTMLPanel height="100%">
		<r:AdvanceCellTable ui:field="table" addStyleNames="{style.standardTable}" />
		</g:HTMLPanel>
	</g:Tab>
	<g:Tab ui:field="tab3">
		<g:TabHTML>MCS Participation</g:TabHTML>
		<g:HTMLPanel height="100%"><g:Label> MCS Participation </g:Label></g:HTMLPanel>
	</g:Tab>
	</g:TabPanel>
</g:HTMLPanel>
 	
</ui:UiBinder> 