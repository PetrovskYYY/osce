<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:r='urn:import:ch.unibas.medizin.osce.client.scaffold.ui' xmlns:a='urn:import:ch.unibas.medizin.osce.client.managed.ui'
	xmlns:my='urn:import:ch.unibas.medizin.osce.client.style.widgets'
	xmlns:b='urn:import:com.google.gwt.user.cellview.client'
	xmlns:n='urn:import:ch.unibas.medizin.osce.client.a_nonroo.client.ui.role'>

	<ui:style src="../../styles.css">
		.topPadding {
			padding-top: 8px;
		}
		
		.panelPadding {
			padding-top: 8px;
			padding-left: 15px;
		}
		
		.textBoxPadding {
			padding-top: 8px;
			padding-left: 40px;
		}
		
		.buttonPadding {
			padding-top: 8px;
			padding-left: 5px;
		}
		
		.arrowUp {
			background-image: url(images/loading.gif);
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:VerticalPanel styleName="{style.panelPadding}" width="200%"
				ui:field="vPanel">
				<g:NotificationMole animationDuration='0'
					message='loading...' ui:field='mole'>
				</g:NotificationMole>
				<g:Image url="/osMaEntry/gwt/unibas/images/loading.gif"></g:Image>

			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
