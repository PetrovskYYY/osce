<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="shortName">
            <div class="{style.label}">Short Name</div>
            <div class="{style.value}" ui:field="shortName"/>
        </div>
    <div id="longName">
            <div class="{style.label}">Long Name</div>
            <div class="{style.value}" ui:field="longName"/>
        </div>
    <div id="caseDescription">
            <div class="{style.label}">Case Description</div>
            <div class="{style.value}" ui:field="caseDescription"/>
        </div>
    <div id="roleScript">
            <div class="{style.label}">Role Script</div>
            <div class="{style.value}" ui:field="roleScript"/>
        </div>
    <div id="roleType">
            <div class="{style.label}">Role Type</div>
            <div class="{style.value}" ui:field="roleType"/>
        </div>
    <div id="roleTopic">
            <div class="{style.label}">Role Topic</div>
            <div class="{style.value}" ui:field="roleTopic"/>
        </div>
    <div id="author">
            <div class="{style.label}">Author</div>
            <div class="{style.value}" ui:field="author"/>
        </div>
    <div id="reviewer">
            <div class="{style.label}">Reviewer</div>
            <div class="{style.value}" ui:field="reviewer"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
