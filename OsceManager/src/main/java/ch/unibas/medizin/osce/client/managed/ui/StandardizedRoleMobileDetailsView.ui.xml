<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='shortName'><div class='{style.label}'>Short Name</div><div class='{style.value}' ui:field='shortName'></div></div>
    <div id='longName'><div class='{style.label}'>Long Name</div><div class='{style.value}' ui:field='longName'></div></div>
    <div id='caseDescription'><div class='{style.label}'>Case Description</div><div class='{style.value}' ui:field='caseDescription'></div></div>
    <div id='roleScript'><div class='{style.label}'>Role Script</div><div class='{style.value}' ui:field='roleScript'></div></div>
    <div id='roleType'><div class='{style.label}'>Role Type</div><div class='{style.value}' ui:field='roleType'></div></div>
    <div id='active'><div class='{style.label}'>Active</div><div class='{style.value}' ui:field='active'></div></div>
    <div id='roleTopic'><div class='{style.label}'>Role Topic</div><div class='{style.value}' ui:field='roleTopic'></div></div>
    <div id='roleParticipants'><div class='{style.label}'>Role Participants</div><div class='{style.value}' ui:field='roleParticipants'></div></div>
    <div id='previousVersion'><div class='{style.label}'>Previous Version</div><div class='{style.value}' ui:field='previousVersion'></div></div>
    <div id='studyYear'><div class='{style.label}'>Study Year</div><div class='{style.value}' ui:field='studyYear'></div></div>
    <div id='mainVersion'><div class='{style.label}'>Main Version</div><div class='{style.value}' ui:field='mainVersion'></div></div>
    <div id='subVersion'><div class='{style.label}'>Sub Version</div><div class='{style.value}' ui:field='subVersion'></div></div>
    <div id='keywords'><div class='{style.label}'>Keywords</div><div class='{style.value}' ui:field='keywords'></div></div>
    <div id='advancedSearchCriteria'><div class='{style.label}'>Advanced Search Criteria</div><div class='{style.value}' ui:field='advancedSearchCriteria'></div></div>
    <div id='simpleSearchCriteria'><div class='{style.label}'>Simple Search Criteria</div><div class='{style.value}' ui:field='simpleSearchCriteria'></div></div>
    <div id='checkList'><div class='{style.label}'>Check List</div><div class='{style.value}' ui:field='checkList'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
