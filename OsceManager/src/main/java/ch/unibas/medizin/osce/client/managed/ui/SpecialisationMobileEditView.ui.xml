<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.unibas.medizin.osce.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}">
                <g:TextBox ui:field="name"/>
            </div>
        </div>
        <div id="roleTopics">
            <div class="{style.label}">Role Topics</div>
            <div class="{style.value}">
                <a:RoleTopicSetEditor ui:field="roleTopics"/>
            </div>
        </div>
        <div id="doctors">
            <div class="{style.label}">Doctors</div>
            <div class="{style.value}">
                <a:DoctorSetEditor ui:field="doctors"/>
            </div>
        </div>
        <div id="oscePostBlueprint">
            <div class="{style.label}">Osce Post Blueprint</div>
            <div class="{style.value}">
                <a:OscePostBlueprintSetEditor ui:field="oscePostBlueprint"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
