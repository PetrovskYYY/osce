<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
      <tr id="type">
                <td>
                    <span class="{style.label}">Type:</span>
                </td>
                <td>
                    <span ui:field="type"/>
                </td>
            </tr>
      <tr id="slotNumber">
                <td>
                    <span class="{style.label}">Slot Number:</span>
                </td>
                <td>
                    <span ui:field="slotNumber"/>
                </td>
            </tr>
      <tr id="timeStart">
                <td>
                    <span class="{style.label}">Time Start:</span>
                </td>
                <td>
                    <span ui:field="timeStart"/>
                </td>
            </tr>
      <tr id="timeEnd">
                <td>
                    <span class="{style.label}">Time End:</span>
                </td>
                <td>
                    <span ui:field="timeEnd"/>
                </td>
            </tr>
      <tr id="osceDay">
                <td>
                    <span class="{style.label}">Osce Day:</span>
                </td>
                <td>
                    <span ui:field="osceDay"/>
                </td>
            </tr>
      <tr id="oscePostRoom">
                <td>
                    <span class="{style.label}">Osce Post Room:</span>
                </td>
                <td>
                    <span ui:field="oscePostRoom"/>
                </td>
            </tr>
      <tr id="student">
                <td>
                    <span class="{style.label}">Student:</span>
                </td>
                <td>
                    <span ui:field="student"/>
                </td>
            </tr>
      <tr id="patientInRole">
                <td>
                    <span class="{style.label}">Patient In Role:</span>
                </td>
                <td>
                    <span ui:field="patientInRole"/>
                </td>
            </tr>
      <tr id="examiner">
                <td>
                    <span class="{style.label}">Examiner:</span>
                </td>
                <td>
                    <span ui:field="examiner"/>
                </td>
            </tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
