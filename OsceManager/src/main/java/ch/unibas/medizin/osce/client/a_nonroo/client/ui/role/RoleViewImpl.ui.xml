<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets" xmlns:s="urn:import:javax.swing" xmlns:t="urn:import:ch.unibas.medizin.osce.client.style.widgetsnewcustomsuggestbox.test.client"  xmlns:impl="urn:import:ch.unibas.medizin.osce.client.style.widgetsnewcustomsuggestbox.test.client.ui.widget.suggest.impl" xmlns:o="urn:import:org.apache.xpath.operations">

	<ui:style src="../../styles.css" />
		<!-- spec start -->
	<g:SplitLayoutPanel ui:field="splitLayoutPanel">
		<g:west size="1225">
			<!--g:ScrollPanel -->
				<g:HTMLPanel ui:field="westPanel">
				  <div class="{style.controls}  {style.controlsleft}">
				 	<my:QuickSearchBox ui:field="searchBox" />
					<my:IconButton ui:field="filterButton" icon="wrench"></my:IconButton>
					<div class="{style.controlsright}">
						<my:IconButton ui:field="exportButton" icon="extlink"/>
					</div>
				</div>
				 
				<b:CellTable ui:field="table" addStyleNames="{style.standardTable}" />
				<div class="{style.controls} {style.controlsleft}">
				 <b:SimplePager ui:field="pager" display="{table}" />
				 </div>
			 	<!-- <div class="{style.controls} {style.controlsleft}"> -->
			
		<!-- <div class="{style.controls}  {style.controlsleft}" > -->
		<div><table><tr><td>
					<g:TextBox ui:field="topicName" />
					<!-- 
					<g:ValueListBox ui:field="traitTypeBox1"></g:ValueListBox>
					-->
					</td><td>
					<g:ListBox ui:field="slots_till_change"></g:ListBox></td>
					<td>
					<g:ValueListBox ui:field="studyYearBox"></g:ValueListBox>
					</td>
					
				<!-- 	<g:ListBox ui:field="SpecialisationId"></g:ListBox>-->
				<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					
				
					<!-- <g:ValueListBox ui:field="SpecialisationBox"></g:ValueListBox> -->
					<td>
					<impl:DefaultSuggestBox ui:field="SpecialisationBox"></impl:DefaultSuggestBox>
				</td>
					<!-- //Issue # 122 : Replace pull down with autocomplete. -->
					<td>
					<my:IconButton ui:field="newButton" icon="plusthick"></my:IconButton>
					</td>
					</tr>
					</table>
				</div>
				<div>
				<!--  <my:CustomSuggestBox ui:field="cSuggestBox" ></my:CustomSuggestBox>
			 	<t:MySuggestionBoxDemo ui:field="suggestdemo"></t:MySuggestionBoxDemo> 
			 	<i:DefaultIconedSuggestBox ui:field="checkdemo"></i:DefaultIconedSuggestBox> --> 
			 <!--  	<impl:DefaultSuggestBox ui:field="defaultsuggest"></impl:DefaultSuggestBox> --> 
				</div>
				
				</g:HTMLPanel>
			<!--g:ScrollPanel -->
		</g:west>
		<g:center>
			<g:ScrollPanel ui:field="scrollPanel">
				<g:SimplePanel ui:field="detailsPanel"></g:SimplePanel>
			</g:ScrollPanel>
		</g:center>
	</g:SplitLayoutPanel>
	
	<!--  spec end-->
</ui:UiBinder> 