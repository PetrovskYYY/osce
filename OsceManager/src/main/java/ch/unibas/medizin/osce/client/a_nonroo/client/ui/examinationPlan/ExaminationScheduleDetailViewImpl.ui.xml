	<g:ScrollPanel ui:field="scrollPanel"  ><!-- Assignment E:Module 5 -->
	
	<!-- h1>Circuit Details Place</h1  -->
	
		<g:VerticalPanel width="100%">
		<g:HorizontalPanel spacing="20" >
		
			<g:HorizontalPanel spacing="10">
			<g:Label ui:field="shortBreakTxt" addStyleNames="{style.label}"></g:Label>
			<g:Label ui:field="shortBreakValue"></g:Label>
			
			<g:Label ui:field="shortBreakSimPatChangeTxt" addStyleNames="{style.label}"></g:Label>
