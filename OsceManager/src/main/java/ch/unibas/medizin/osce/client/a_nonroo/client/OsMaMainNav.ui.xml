				<g:Anchor ui:field="course">Postenlauf</g:Anchor>
				<g:Anchor ui:field="students">Studenten</g:Anchor>
				<g:Anchor ui:field="examinationSchedule">Prüfungsplan</g:Anchor>
				<g:Anchor ui:field="printIndividualSchedules">Individuelle Pläne drucken</g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>
		
		<g:DisclosurePanel ui:field="simulationPatientsPanel" open="true" addStyleNames='{style.mainNavPanel}'>
