<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:s="urn:import:ch.unibas.medizin.osce.client.style.widgets">
<ui:style src="styles.css" >

.leftbar{
	
	padding: 0px 5px 3px 5px;
	border-color: #AAAAAA;
	border-style: solid; 
	border-right-width: 0px;
	border-top-width: 0px;
	border-left-width: 0px;
	border-bottom-width: 0px;
}	

.closeButton{
	
	float: right;
	margin-right: 2px;
}

.headerLabel{
	vertical-align: middle;
	margin: 6px 0 0 12px;
}

</ui:style>

	<g:HTMLPanel>
	
	<g:HorizontalPanel addStyleNames='{style.mainNavPanel}' >
	
		<g:DockPanel ui:field="dockPanel" addStyleNames='{style.leftbar}' >		
		</g:DockPanel>
		
		<g:VerticalPanel ui:field="menuContainer">
		<g:DisclosurePanel ui:field="simPatDataPanel" width="182px" open="true" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="simPatDataPanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="simPatClose" ></s:IconButton>
				</g:HorizontalPanel>
			</g:customHeader>
			<!-- Style name für VerticalPanel -->
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
				<g:Anchor ui:field="people"></g:Anchor>
				<g:Anchor ui:field="scars"></g:Anchor>
				<g:Anchor ui:field="anamnesisChecks"></g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>
		
		<g:DisclosurePanel ui:field="doctorDataPanel" width="182px" open="false" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="doctorDataPanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="doctorClose" />
				</g:HorizontalPanel>
			</g:customHeader>
			<!-- Style name für VerticalPanel -->
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
				<g:Anchor ui:field="clinics"></g:Anchor>
				<g:Anchor ui:field="doctors"></g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>

		<!-- By Spec Start -->
		<g:DisclosurePanel ui:field="rolePanel" width="182px" open="true" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="rolePanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="roleClose" />
				</g:HorizontalPanel>
			</g:customHeader>
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
				<g:Anchor ui:field="roles"></g:Anchor>
				<g:Anchor ui:field="topicsAndSpec"></g:Anchor>
				<g:Anchor ui:field="roleScriptTemplate"></g:Anchor>
				<g:Anchor ui:field="roomMaterials"></g:Anchor>
				
			</g:VerticalPanel>
		</g:DisclosurePanel>
		<!-- By SPec End -->
		
		
		
		<g:DisclosurePanel ui:field="examinationsPanel" width="182px" open="true" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="examinationsPanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="examinationsClose" />
				</g:HorizontalPanel>
			</g:customHeader>
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
			<!-- G: SPEC START = -->						
			<g:Label ui:field="labelSemester">Semester:</g:Label>				
			<g:HorizontalPanel>
				<g:ValueListBox ui:field="lstSemester" />				
				 <s:IconButton ui:field="btnEditSem" icon="pencil"></s:IconButton>
				<s:IconButton ui:field="btnAddSem" icon="plusthick"></s:IconButton> 
			</g:HorizontalPanel> 
		<!-- G: SPEC END = -->	
				<g:Anchor ui:field="importeOSCE"></g:Anchor>
				<g:Anchor ui:field="osces"></g:Anchor>
				<g:Anchor ui:field="circuit"></g:Anchor>
				<g:Anchor ui:field="students"></g:Anchor>
				<g:Anchor ui:field="examinationSchedule"></g:Anchor>
				<g:Anchor ui:field="summonings"></g:Anchor>
				<g:Anchor ui:field="individualSchedules"></g:Anchor>
				<g:Anchor ui:field="bellSchedule"></g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>
	
                <!-- module: 3  SPEC START = -->
		<g:DisclosurePanel ui:field="roleAssignmentPanel" width="182px" open="true" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="roleAssignmentPanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="roleAssignmentClose" />
				</g:HorizontalPanel>
			</g:customHeader>
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
				<g:Anchor ui:field="roleAssignment"></g:Anchor>				
			</g:VerticalPanel>
		</g:DisclosurePanel>
		<!-- module: 3  SPEC END = -->
		
		<g:DisclosurePanel ui:field="administrationPanel" width="182px" open="false" addStyleNames='{style.mainNavPanel}'>
			<g:customHeader>
				<g:HorizontalPanel>
					<g:Label ui:field="administrationPanelHeaderText" addStyleNames="{style.headerLabel}" ></g:Label>
					<s:IconButton icon="arrowreturnthick-1-w" addStyleNames='{style.closeButton}' ui:field="administrationClose" />
				</g:HorizontalPanel>
			</g:customHeader>
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
				<g:Anchor ui:field="administrators"></g:Anchor>
				<g:Anchor ui:field="nationalities"></g:Anchor>
				<g:Anchor ui:field="languages"></g:Anchor>
				<g:Anchor ui:field="professions"></g:Anchor>
				<g:Anchor ui:field="rooms"></g:Anchor>
				<g:Anchor ui:field="log"></g:Anchor>
				<g:Anchor ui:field="importObjective"></g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>
		
		<!-- Commented by spec 
		<g:DisclosurePanel ui:field="simulationPatientsPanel" open="true" addStyleNames='{style.mainNavPanel}'>
			<g:header></g:header>
			<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}'>
			 <g:Anchor ui:field="roles"></g:Anchor>
			 	
				<g:Anchor ui:field="roleAssignments"></g:Anchor>
			</g:VerticalPanel>
		</g:DisclosurePanel>
-->	
		
	</g:VerticalPanel>
		
</g:HorizontalPanel>
	</g:HTMLPanel>


</ui:UiBinder> 
