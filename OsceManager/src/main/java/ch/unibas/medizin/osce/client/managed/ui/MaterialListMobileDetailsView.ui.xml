<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
        <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}" ui:field="name"/>
        </div>
        <div id="type">
            <div class="{style.label}">Type</div>
            <div class="{style.value}" ui:field="type"/>
        </div>
        <div id="price">
            <div class="{style.label}">Price</div>
            <div class="{style.value}" ui:field="price"/>
        </div>
        <div id="priceType">
            <div class="{style.label}">Price Type</div>
            <div class="{style.value}" ui:field="priceType"/>
        </div>
        <div id="usedMaterials">
            <div class="{style.label}">Used Materials</div>
            <div class="{style.value}" ui:field="usedMaterials"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
