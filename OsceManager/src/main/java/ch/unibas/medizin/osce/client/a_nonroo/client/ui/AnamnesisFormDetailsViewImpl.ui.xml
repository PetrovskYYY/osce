<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets">
<ui:style src="../styles.css" />

  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
      <tr id="createDate">
                <td>
                    <span class="{style.label}">Create Date:</span>
                </td>
                <td>
                    <span ui:field="createDate"/>
                </td>
            </tr>
      <tr id="anamnesischecksvalues">
                <td>
                    <span class="{style.label}">Anamnesischecksvalues:</span>
                </td>
                <td>
                    <span ui:field="anamnesischecksvalues"/>
                </td>
            </tr>
      <tr id="scars">
                <td>
                    <span class="{style.label}">Scars:</span>
                </td>
                <td>
                    <span ui:field="scars"/>
                </td>
            </tr>
    </table>

  <div class="{style.bar}">
    <my:IconButton icon="pencil" addStyleNames="{style.button}" ui:field="edit">Edit</my:IconButton>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
